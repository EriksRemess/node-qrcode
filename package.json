{
  "name": "qrcode",
  "description": "QRCode / 2d Barcode API for Node.js",
  "version": "1.5.4",
  "author": "Ēriks Remess <eriks@remess.lv>",
  "contributors": [
    "Ryan Day <soldair@gmail.com>",
    "Vincenzo Greco <greco.vincenzo@gmail.com>",
    "Linus Unnebäck <linus@folkdatorn.se>"
  ],
  "keywords": [
    "qr",
    "code",
    "canvas",
    "qrcode"
  ],
  "type": "module",
  "main": "./lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./helper/to-sjis": "./helper/to-sjis.js",
    "./package.json": "./package.json"
  },
  "imports": {
    "#examples/*": "./examples/*.js",
    "#helper/*": "./helper/*.js",
    "#lib/*": "./lib/*.js",
    "#test/*": "./test/*.js"
  },
  "files": [
    "lib",
    "helper"
  ],
  "homepage": "http://github.com/EriksRemess/node-qrcode",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node --throw-deprecation test.js",
    "bench:svg-string": "node examples/bench-svg-tostring.js",
    "bench:image-data": "node examples/bench-image-data.js",
    "bench:all": "node examples/bench-all.js",
    "bench:hotspots": "node examples/bench-hotspots.js"
  },
  "dependencies": {
    "pngjs": "^7.0.0"
  },
  "devDependencies": {
    "canvas": "^3.2.1",
    "eslint": "^10.0.0",
    "globals": "^17.3.0",
    "htmlparser2": "^10.1.0",
    "jsqr": "^1.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/EriksRemess/node-qrcode.git"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
